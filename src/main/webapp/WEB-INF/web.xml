<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletLogin</servlet-name>
  	<display-name>ServletLogin</display-name>
  	<description></description>
  	<servlet-class>uy.turismo.webapp.servlets.ServletLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletRegister</servlet-name>
  	<display-name>ServletRegister</display-name>
  	<description></description>
  	<servlet-class>uy.turismo.webapp.servlets.ServletRegister</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletProfile</servlet-name>
  	<display-name>ServletProfile</display-name>
  	<description></description>
  	<servlet-class>uy.turismo.webapp.servlets.ServletProfile</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletLogin</servlet-name>
  	<url-pattern>/ServletLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletRegister</servlet-name>
  	<url-pattern>/ServletRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletProfile</servlet-name>
  	<url-pattern>/profile</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>
        <welcome-file>/pages/home/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
